<%- include('header') %>

    <style>
        .proImgList img {
            height: 122px;
            object-fit: cover;
        }

        .commodity_div {
            background: #efefef;
            border-radius: 20px;
        }

        .image_div {
            background: #fff;
            width: 90px;
            height: 90px;
            margin: 10px auto;
            border-radius: 60%;
            text-align: center;
        }

        .image_div img {
            margin-top: 18px;
            width: 70px;
        }

        .commodity_name {
            font-weight: 600;
            text-align: center;
            font-size: 16px;
            color: #3a3a3a;
        }

        .commodity_price_div {
            text-align: center;
            color: #5a5a5a;
        }

        .commodity_quantity {
            color: #ff8d00;
        }
    </style>

    <!-- mid-part start -->
    <div class="main-panel">
        <div class="contentright w-auto">
            <div class="container-fluid mt-3">
                <div class="row">
                    <div class="col-md-10">
                        <div class="d-flex">
                            <div class="tableheading ">
                                <h3>Fetch Shipment Rate</h3>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-2">
                        <div class="searchbox float-right">
                            <div class="form-group has-search">
                                <a href="/shipment" class="btn btn-dark mt-2">Back</a>
                            </div>
                        </div>
                    </div>
                </div>


                <!-- Start -->
                <div class="row">
                    <form class="pt-3" id="formData">
                        <input type="hidden" name="_csrf" id="csrf" value="">
                        <input type="hidden" id="id" value="<%= shipmentDetail.id%>">

                        <div class="form-group" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                              <span class="input-group-text">Commodity Name</i></span>
                              </div>
                                <input type="text" class="form-control " id="cname" name="commodityName" value="<%= shipmentDetail.commodity_name %>" disabled>
                            </div>
                        </div>

                        <div class="form-group" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                              <span class="input-group-text">Commodity Weight</i></span>
                              </div>
                                <input type="text" class="form-control " id="cweight" name="commodityWeight" value="<%= shipmentDetail.quantity %>" disabled>
                            </div>
                        </div>



                        <div class="form-group" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                              <span class="input-group-text">Source Address</i></span>
                              </div>
                                <input type="text" class="form-control " id="saddress" name="sourceAddress" placeholder="Source Address" value="4009 Marathon Blvd, Suite 300 Austin US" disabled>
                            </div>
                        </div>

                        <div class="form-group" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                              <span class="input-group-text">Destination Address line-1</i></span>
                              </div>
                                <input type="text" class="form-control " id="daddress" name="destinationAddress" placeholder="Destination Address" value="<%= shipmentDetail.address %>" disabled>
                            </div>
                        </div>

                        <!-- <div class="form-group" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                              <span class="input-group-text"></i></span>
                              </div>
                                <input type="text" class="form-control " id="saddress" name="sourceAddress" placeholder="Address (optional)" >
                            </div>
                        </div> -->

                        <div class="form-group col-md-4 col-xs-12" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                              <span class="input-group-text">City</i></span>
                              </div>
                                <input type="text" class="form-control " id="saddress" name="sourceAddress" placeholder="City" value="<%= shipmentDetail.city %>" disabled>
                            </div>
                        </div>

                        <div class="form-group col-md-4 col-xs-12" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                              <span class="input-group-text">State</i></span>
                              </div>
                                <input type="text" class="form-control " id="saddress" name="sourceAddress" placeholder="State" value="<%= shipmentDetail.state %>" disabled>
                            </div>
                        </div>

                        <div class="form-group col-md-4 col-xs-12" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                              <span class="input-group-text">Postal Code</i></span>
                              </div>
                                <input type="text" class="form-control " id="saddress" name="sourceAddress" placeholder="Pin Code" value="<%= shipmentDetail.pin_code %>" disabled>
                            </div>
                        </div>

                       


                        <div class="form-group" id="mailErr">
                            <div class="input-group col-md-4 col-xs-12">
                                <div class="input-group-prepend">
                              <span class="input-group-text">Parcel length(in inch)</i></span>
                              </div>
                                <input type="text" class="form-control " id="pLength" name="parcelLength" placeholder="" >
                            </div>
                        </div>

                        <div class="form-group col-md-4 col-xs-12" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">Parcel width(in inch)</i></span>
                              </div>
                                <input type="text" class="form-control " id="pWidth" name="parcelWidth" placeholder="" >
                            </div>
                        </div>

                        <div class="form-group col-md-4 col-xs-12" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">Parcel height(in inch)</i></span>
                              </div>
                                <input type="text" class="form-control " id="pHeight" name="parcelHeight" placeholder="" >
                            </div>
                        </div>

                        <div class="form-group" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">Parcel Weight(in gram)</i></span>
                              </div>
                                <input type="text" class="form-control " id="pWeight" name="parcelHeight" placeholder="" >
                            </div>
                        </div>

                        <!-- <div class="form-group" id="mailErr">
                            <div class="input-group">
                                <div class="input-group-prepend">
                              <span class="input-group-text"></i></span>
                              </div>
                                <input type="text" class="form-control " id="pPrice" name="parcelPrice" placeholder="Parcel Price" >
                            </div>
                        </div> -->

                        
                        <div class="mt-3">
                            <a class="btn btn-block btn-primary btn-lg
                            font-weight-medium auth-form-btn loginBtn" type="button" id="shipmentForm"><b>Fetch Rate</b></a>
                        </div>
                    </form>
                </div>
                <!-- End -->
            </div>
        </div>

        <!-- footer section start -->
        <%- include('footer') %>

<script>

        // Delete intent by ajax. 
        $('#shipmentForm').click(function () {
const id = $('#id').val()
console.log("id",id);
            const formData = new FormData();
            formData.append("p_length",$('#pLength').val())
            formData.append("p_width",$('#pWidth').val())
            formData.append("p_height",$('#pHeight').val())
            formData.append("p_weight",$('#pWeight').val())
            formData.append("rate_id", id)

            $.ajax({
                type: "post",
                url: `/update-shipment-detail/${id}`,
                data: formData,
                dataType: 'JSON',
                //    cache : false,
                contentType: false,
                processData: false,
                success: function (response) {
                    console.log("response", response);
                    if (response.status == "success") {
                        toastr.remove();
                        toastr.success(response.message);
                        window.setTimeout(function () {
                            
                            // window.location.reload();
                            window.location = `/shipping-detail/${response.id}`;
                            
                        }, 1000);
                    }
                },
                error: function (response) {
                    let errorResponse = JSON.parse(response.responseText)
                    toastr.remove();
                    toastr.error(errorResponse.message);
                }
            });
        });
    </script>